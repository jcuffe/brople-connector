{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "process": "readonly"
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "semi": ["error", "never"]
  }
}

